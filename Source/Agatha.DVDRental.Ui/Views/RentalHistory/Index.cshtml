@using Agatha.DVDRental.Subscription.Model.RentalHistory
@model System.Collections.Generic.IEnumerable<Agatha.DVDRental.Subscription.Model.RentalHistory.Rental>

<div class="row-fluid">  
    <div class="span9">
         @foreach (Rental filmRented in Model)
         {
             <h3>DvdId: @filmRented.DvdId</h3>
             <img src="@Url.Content("~/Content/images/noimage-140x200.png")" />
             <p>DateSentOut: @filmRented.DateSentOut</p>
             <p>DateReturned: @filmRented.DateReturned</p>   
             <hr/>
         } 
    </div>
    
